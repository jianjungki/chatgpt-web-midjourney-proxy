{
	"name": "chatgpt-web-midjourney-proxy",
	"version": "2.22.8",
	"private": false,
	"description": "ChatGPT Web Midjourney Proxy",
	"author": "Dooy <ydlhero@gmail.com>",
	"keywords": [
		"chatgpt-web",
		"chatgpt",
		"chatbot",
		"Midjourney",
		"Midjourney UI",
		"Midjourney Proxy",
		"gpts",
		"gpts ui",
		"vue"
	],
	"scripts": {
		"dev": "vite",
		"format": "prettier --write src/",
		"build": "run-p build-only",
		"buildold": "run-p type-check build-only",
		"preview": "vite preview",
		"build-only": "vite build",
		"type-check": "vue-tsc --noEmit",
		"lint": "eslint --ext .vue,.mjs,.ts,.tsx --fix src/ --ignore-path .gitignore",
		"lint:fix": "eslint --ext .ts,.vue --ignore-path .gitignore --fix src/ --fix",
		"bootstrap": "pnpm install && pnpm run common:prepare",
		"common:cleanup": "rimraf node_modules && rimraf pnpm-lock.yaml",
		"common:prepare": "husky install"
	},
	"dependencies": {
		"@ffmpeg/ffmpeg": "^0.12.10",
		"@ffmpeg/util": "^0.12.1",
		"@traptitech/markdown-it-katex": "^3.6.0",
